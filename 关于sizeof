#include <stdio.h>

int main(){
    char *p;
    char p1[5];
    char **p2;
    char *s="1234";
    char s1[] = "1234";
    char **s2 = "1234";


    printf("p=%d\n",sizeof(p));    
    printf("p1=%d\n",sizeof(p1));
    printf("p2=%d\n",sizeof(p2));
    printf("s=%d\n",sizeof(s));
    printf("s1=%d\n",sizeof(s1));
    printf("s2=%d\n",sizeof(s2));

}
